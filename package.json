{
  "name": "@costar/apts-web-components",
  "version": "0.2.2",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/mortar/mortar.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "static": "gatsby build",
    "serve": "gatsby serve",
    "dev": "node ./icons.js && stencil build --dev --watch --serve",
    "build": "node ./icons.js && stencil build --docs && webpack",
    "start": "npm-run-all build develop",
    "icons": "node ./icons.js",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "npm-run-all build static",
    "component": "stencil generate",
    "webpack": "webpack",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@stencil/angular-output-target": "0.0.5",
    "@stencil/core": "^1.12.2",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "core-js": "^3.6.5",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "standard-version": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dom-focus-lock": "^1.0.4",
    "gatsby": "^2.24.51",
    "gatsby-plugin-copy-files-enhanced": "^1.1.1",
    "gatsby-plugin-less": "^4.0.3",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-source-filesystem": "^2.3.26",
    "gatsby-transformer-remark": "^2.8.30",
    "markdown-to-jsx": "^6.11.4",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-live": "^2.2.2",
    "react-mermaid2": "^0.1.4",
    "text-mask-addons": "^3.8.0",
    "typescript": "^3.8.3",
    "vanilla-text-mask": "^5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://tfs.prd.costargroup.com/CoStarCollection/Apartments/_git/apts-web-components"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
